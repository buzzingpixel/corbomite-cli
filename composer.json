{
    "name": "buzzingpixel/corbomite-cli",
    "description": "Corbomite CLI",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "vlucas/phpdotenv": "^2.5",
        "php-di/php-di": "^6.0",
        "nunomaduro/collision": "^2.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
