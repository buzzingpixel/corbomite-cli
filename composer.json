{
    "name": "buzzingpixel/corbomite-cli",
    "description": "Corbomite CLI",
    "version": "1.0.3",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "nunomaduro/collision": "^2.1",
        "composer/composer": "^1.8",
        "buzzingpixel/corbomite-di": "^1.0",
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "cliActionConfigFilePath": "src/actionConfig.php",
        "diConfigFilePath": "src/diConfig.php"
    }
}
