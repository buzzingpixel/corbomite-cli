{
    "name": "buzzingpixel/corbomite-cli",
    "description": "Corbomite CLI",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "vlucas/phpdotenv": "^2.5",
        "nunomaduro/collision": "^2.1",
        "psr/container": "^1.0",
        "composer/composer": "^1.8"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "php-di/php-di": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "cliActionConfigFilePath": "src/actionConfig.php"
    }
}
